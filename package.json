{
  "name": "templates",
  "version": "0.0.0",
  "private": true,
  "main": "./bin/www",
  "scripts": {
    "start": "nodemon --ignore public/ ./bin/www",
    "clean": "rm -rf ./cache",
    "prod": "NODE_ENV=production node ./bin/www",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:html",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . views/*",
    "lint:js:fix": "eslint . views/* --fix",
    "lint:css": "stylelint */**/*.ejs */**/*.css",
    "lint:css:fix": "stylelint */**/*.ejs */**/*.css --fix",
    "lint:html": "node htmllint"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "es6": "0.0.7",
    "express": "^4.17.1",
    "express-minify": "^1.0.0",
    "favicon": "0.0.2",
    "google-auth-library": "^0.12.0",
    "google-spreadsheet": "^2.0.4",
    "googleapis": "^23.0.0",
    "html-minifier": "^3.5.9",
    "medium-get-latest-posts": "^1.1.2",
    "morgan": "~1.9.0",
    "node-instagram": "^4.1.0",
    "request": "latest",
    "sendmail": "^1.2.0",
    "serve-favicon": "~2.4.5",
    "slack-node": "^0.1.8",
    "twitter": "^1.7.1",
    "uglify-es": "^3.3.10"
  },
  "devDependencies": {
    "@gerhobbelt/xregexp": "^3.2.0-22",
    "chalk": "^2.3.1",
    "eslint": "^4.17.0",
    "eslint-plugin-ejs": "0.0.2",
    "eslint-plugin-script-tags": "^0.5.0",
    "glob": "^7.1.2",
    "htmllint": "^0.7.0",
    "nodemailer": "^6.4.16",
    "nodemon": "^1.19.4",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0"
  }
}
